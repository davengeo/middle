{
  "fileLog": {
    "level": "debug",
    "filename": "./logs/default-logs.log",
    "handleExceptions": true,
    "json": true,
    "timestamp": true,
    "maxsize": 5242880,
    "maxFiles": 5,
    "colorize": false
  },
  "consoleLog": {
    "level": "debug",
    "handleExceptions": true,
    "json": false,
    "colorize": true,
    "humanReadableUnhandledException": true,
    "timestamp": true
  },
  "config-1": {
    "routing": {
      ".*/test1": "http://localhost:3001/"
    },
    "default": "http://localhost:3000",
    "port": 4010
  },
  "config-err": {
    "routing": {
      ".*/test1": "http://localhost:3001/"
    },
    "default": "http://localhost:3000",
    "port": "bad-port"
  }
}
